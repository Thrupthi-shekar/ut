<script type="text/javascript">
    $(function () {
        getMorrisDocument('donut', 'document_level_donut_chart');
        getMorrisSentence('donut', 'sentence_level_donut_chart');
    });


    function getMorrisDocument(type, element) {
        if (type === 'donut') {
            Morris.Donut({
                element: element,
                data: [
                    {
                        label: 'Positive',
                        value: {{documentPositive}}
                    }, {
                        label: 'Negative',
                        value: {{documentNegative}}
                    }, {
                        label: 'Neutral',
                        value: {{documentNeutral}}
                    }],
                colors: ['#27B04B', '#D03D2E', '#4C4848'],
                formatter: function (y) {
                    return y + '%'
                }
            });
        }
    }
    function getMorrisSentence(type, element) {
        if (type === 'donut') {
            Morris.Donut({
                element: element,
                data: [
                    {
                        label: 'Positive',
                        value: {{sentencePositive}}
                    }, {
                        label: 'Negative',
                        value: {{sentenceNegative}}
                    }, {
                        label: 'Neutral',
                        value: {{sentenceNeutral}}
                    }],
                colors: ['#7CBC2A', '#BC2A63', '#756F6F'],
                formatter: function (y) {
                    return y + '%'
                }
            });
        }
    }
</script>